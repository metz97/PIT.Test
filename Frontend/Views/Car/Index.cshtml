@model IEnumerable<Frontend.Models.CarModels>
@{
    ViewBag.Title = "Car";
}

<div class="container">
    @foreach (var item in Model)
    {
        <img src="@Url.Action("GetImages","Car", new { id = item.Id })" alt="@item.Name" class="img-fluid" height="200"
             width="200">
    }
</div>
